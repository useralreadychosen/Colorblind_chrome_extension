{
    "name": "Color-Blind Friend",
    "description": "Change colors to be better for colorblind users",
    "version": "0.1",
    "permissions": [
        "tabs","<all_urls>"
    ],
    "options_page": "./options.html",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "manifest_version": 2
}