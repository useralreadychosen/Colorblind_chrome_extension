{
    "name": "Colorblind",
    "description": "Change colors to be better for colorblind users",
    "version": "0.1",
    "permissions": [
        "tabs","<all_urls>"
    ],
    "browser_action": {
        "default_icon": "icon.png"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "run_at": "document_end",
        "js": ["injected.js"],
        "css": ["injected.css"]
    }], 
    "manifest_version": 2
}